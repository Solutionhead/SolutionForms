@model SolutionForms.Client.Mvc.ViewModels.DataForms.DataFormsIndexViewModel

<div class="container">
  <div class="col-md-6 col-lg-3">
    <h2>@Model.OrganizationName</h2>
    <h3>My Apps</h3>

    <ul class="nav nav-stacked nav-pills">
      <li class="bg-info">
        @Html.ActionLink("Create new form", "New", "Dataforms")
      </li>

      @foreach (var item in Model.Forms)
      {
          <li>
            <a href="@item.Url">@item.Title</a>
          </li>
      }
    </ul>
  </div>
</div>